{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "lib": ["es6", "dom"],
      "types": ["cypress", "node"],
      "allowJs": true,
      "noEmit": true,
      "strict": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["**/*.ts", "cypress.config.ts"]
  }
  